		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"gh-pages": "^5.0.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.0",
